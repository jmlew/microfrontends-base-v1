{
  "name": "microfr",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "npm run build && npm run serve",
    "------ SERVE: DIST FILES ------": "",
    "serve": "npm run serve:shell & npm run serve:client-red & npm run serve:client-blue & npm run serve:client-orange",
    "serve:shell": "live-server dist/apps/mfe-shell --no-browser --port=6600",
    "serve:clients": "npm run serve:client-red & npm run serve:client-blue & npm run serve:client-orange",
    "serve:client-red": "live-server dist/apps/mfe-client-red --no-browser --port=6610",
    "serve:client-blue": "live-server dist/apps/mfe-client-blue --no-browser --port=6620",
    "serve:client-orange": "live-server dist/apps/mfe-client-orange --no-browser --port=6630",
    "------ SERVE: DEV ------": "",
    "serve:dev:shell": "nx serve --project=mfe-shell --port=6601 & npm run serve:clients",
    "serve:dev:client-red": "nx serve --project=mfe-client-red --port=6611",
    "serve:dev:client-blue": "nx serve --project=mfe-client-blue --port=6621",
    "serve:dev:client-orange": "nx serve --project=mfe-client-orange --port=6631",
    "------ BUILD ------": "",
    "build": "npm run build:shell && npm run build:clients",
    "build:shell": "nx build --project=mfe-shell",
    "build:clients": "npm run build:client-red & npm run build:client-blue & npm run build:client-orange",
    "build:client-red": "nx build --project=mfe-client-red --prod --extra-webpack-config=webpack-ng.config.js --single-bundle --output-hashing=none --vendor-chunk=false",
    "build:client-blue": "nx build --project=mfe-client-blue --prod --output-hashing=none --vendor-chunk=false",
    "build:client-orange": "nx build --project=mfe-client-orange --prod --extra-webpack-config=webpack-ng.config.js --single-bundle --output-hashing=none --vendor-chunk=false",
    "------ TEST ------": "",
    "test": "nx test",
    "e2e": "nx e2e",
    "------ CODE ------": "",
    "lint": "nx workspace-lint && nx lint",
    "affected": "nx affected",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "------ CLEANUP ------": "",
    "npmremove": "rm -rf node_modules",
    "clean:install": "npm run npmremove && npm cache verify && npm install",
    "------ MISC ------": "",
    "clean:dist": "rm -r dist/*",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.0",
    "@angular/cdk": "^8.2.3",
    "@angular/cli": "^8.3.29",
    "@angular/common": "^8.2.0",
    "@angular/compiler": "^8.2.0",
    "@angular/core": "^8.2.0",
    "@angular/elements": "^8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.0",
    "@angular/platform-browser-dynamic": "^8.2.0",
    "@angular/router": "^8.2.0",
    "@babel/runtime": "^7.15.4",
    "@material-ui/core": "^4.12.3",
    "@nrwl/nx": "^7.8.7",
    "core-js": "^2.6.12",
    "document-register-element": "1.13.1",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2",
    "rxjs": "~6.4.0",
    "styled-components": "4.4.1",
    "tslib": "^1.14.1",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.4.1",
    "@angular-devkit/build-angular": "^0.803.29",
    "@angular/compiler-cli": "^8.2.0",
    "@angular/language-service": "^8.2.0",
    "@babel/preset-react": "7.7.4",
    "@nrwl/angular": "^8.12.11",
    "@nrwl/cypress": "^8.12.11",
    "@nrwl/eslint-plugin-nx": "8.11.0",
    "@nrwl/jest": "8.11.0",
    "@nrwl/nest": "^8.12.11",
    "@nrwl/react": "^8.12.11",
    "@nrwl/web": "^8.12.11",
    "@nrwl/workspace": "8.11.0",
    "@testing-library/react": "9.4.0",
    "@types/jest": "24.0.9",
    "@types/node": "~8.9.4",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "5.1.3",
    "@types/styled-components": "4.4.1",
    "@typescript-eslint/eslint-plugin": "2.3.2",
    "@typescript-eslint/parser": "2.3.2",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "codelyzer": "~5.0.1",
    "cypress": "3.6.1",
    "dotenv": "6.2.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "jest": "24.1.0",
    "jest-preset-angular": "7.0.0",
    "live-server": "^1.2.1",
    "ngx-build-plus": "^9.0.6",
    "prettier": "1.18.2",
    "rxjs-tslint": "^0.1.8",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "~3.5.3"
  }
}
