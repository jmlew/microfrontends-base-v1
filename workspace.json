            ],
            "exclude": ["**/node_modules/**", "!apps/mfe-client-a/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/mfe-client-a/jest.config.js",
            "tsConfig": "apps/mfe-client-a/tsconfig.spec.json",
            "setupFile": "apps/mfe-client-a/src/test-setup.ts"
          }
        }
      }
    },
    "mfe-client-a-e2e": {
